<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data>
		<record id="qualsys_attendees_views_form" model="ir.ui.view">
			<field name="name">qualsys.attendees.views.form</field>
			<field name="model">qualsys.attendees</field>
			<field name="arch" type="xml">
				<form string="Asistente">
					<sheet>
						<group>
							<field name="partner_id"/>
							<field name="age"/>
						</group>
					</sheet>
				</form>	
			</field>
		</record>

		<record id="qualsys_attendees_views_tree" model="ir.ui.view">
			<field name="name">qualsys.attendees.views.tree</field>
			<field name="model">qualsys.attendees</field>
			<field name="arch" type="xml">
				<tree string="Asistentes">
					<field name="partner_id"/>
					<field name="age"/>
				</tree>	
			</field>
		</record>
		
		<act_window id="qualsys_attendees_action" name="Asistentes" res_model="qualsys.attendees" view_mode="tree,form"/>
		
		<menuitem id="qualsys_attendees_menu" name="Asistentes" action="qualsys_attendees_action" parent="qualsys_school_root"/>
	</data>
</odoo>